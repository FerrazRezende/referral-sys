{
  "name": "referral-system-frontend",
  "version": "1.0.0",
  "description": "Teste técnico | Indicação com árvore binária",
  "scripts": {
    "dev": "tailwindcss -i ./src/css/input.css -o ./public/css/style.css --watch",
    "build": "tailwindcss -i ./src/css/input.css -o ./public/css/style.css --minify && npm run js-build",
    "js-dev": "cat src/js/*.js > public/js/main.js",
    "js-build": "cat src/js/*.js | terser -o public/js/main.js"
  },
  "devDependencies": {
    "@tailwindcss/cli": "next",
    "tailwindcss": "next",
    "terser": "^5.24.0"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "axios": "^1.6.0",
    "sweetalert2": "^11.7.32",
    "chart.js": "^4.4.0",
    "d3": "^7.8.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "keywords": [
    "referral",
    "binary-tree",
    "php",
    "tailwind",
    "jquery",
    "ajax"
  ]
}